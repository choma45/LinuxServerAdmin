bind -m emacs '"\eOd": backward-word'
bind -m emacs '"\e[5D": backward-word'
bind -m emacs '"\e[1~": beginning-of-line'
bind -m emacs '"\e[4~": end-of-line'
bind -m emacs '"\e[8~": end-of-line'
bind -m emacs '"\eOc": forward-word'
bind -m emacs '"\e[5C": forward-word'
bind -m emacs '"\e[5~": history-search-backward'
bind -m emacs '"\e[6~": history-search-forward'
